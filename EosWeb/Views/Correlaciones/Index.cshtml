@section Scripts{
    <script>
        $(document).ready(function () {
            var Tmin = '@ViewBag.Tmin_k';
            var Tmax = '@ViewBag.Tmax_k';
            var Tc = '@ViewBag.Tc_K';

            $("#correlacion").change(function() { 
                if($(this).val() < Tmax) {
                    $("#enfasis").css("background-color","#E74C3C");
                }
                else if($(this).val() >= Tmax && $(this).val() <= Tmin) {
                    $("#enfasis").css("background-color","inherit");
                }
                else if($(this).val() > Tmin && $(this).val() < Tc) {
                    $("#enfasis").css("background-color","#F39C12");
                }
                else if($(this).val() > Tc) {
                    $("#enfasis").css("background-color","#E74C3C");
                }
            });

            $('#btnCC').click(function () {
                //Open modal dialog for edit event
                var t1 = document.getElementsByName("TCal1")[0].value;
                var t2 = document.getElementsByName("TCal2")[0].value;
                var id = '@ViewBag.IdCompuesto';

                $.ajax({
                    type: "POST",
                    url: '/Correlaciones/CapacidadCaloricaCPH/',
                    data: { 'T1': t1, 'T2': t2 , 'Id': id },
                    success: function (data) {
                        if (data != 0) {
                            document.getElementsByName('CPH')[0].setAttribute("value", data);
                            BuscarConstante();
                        }
                        else {
                            alert('No pudo calcularse el valor debido a que se encuentra fuera de los parámetros');
                        }

                    },
                    error: function (f) {
                        console.log(f);
                        alert('No pudo calcularse el valor');
                    }
                })

                $.ajax({
                    type: "POST",
                    url: '/Correlaciones/CapacidadCaloricaCPS/',
                    data: { 'T1': t1, 'T2': t2 , 'Id': id },
                    success: function (data) {
                        if (data != 0) {
                            document.getElementsByName('CPS')[0].setAttribute("value", data);
                        }
                        else {
                            alert('No pudo calcularse el valor debido a que se encuentra fuera de los parámetros');
                        }

                    },
                    error: function (f) {
                        console.log(f);
                        alert('No pudo calcularse el valor');
                    }
                })

            })

            function BuscarConstante() {
                var id = '@ViewBag.IdCompuesto';

                $.ajax({
                    type: "POST",
                    url: '/Correlaciones/LeerConstante/',
                    data: {'Id': id },
                    success: function (data) {
                        console.log(data);
                        document.getElementsByName('CA')[0].setAttribute("value", data.a);
                        document.getElementsByName('CA')[1].setAttribute("value", data.a);
                        document.getElementsByName('CB')[0].setAttribute("value", data.b);
                        document.getElementsByName('CB')[1].setAttribute("value", data.c);
                        document.getElementsByName('CC')[0].setAttribute("value", data.c);
                        document.getElementsByName('CC')[1].setAttribute("value", data.c);
                        document.getElementsByName('CD')[0].setAttribute("value", data.d);
                        document.getElementsByName('CD')[1].setAttribute("value", data.d);

                        document.getElementsByName('CompuestoNombre')[0].setAttribute("value", data.i.compuesto);
                        document.getElementsByName('CompuestoNombre')[1].setAttribute("value", data.i.compuesto);
                        
                    },
                    error: function (f) {
                        console.log(f);
                    }
                })
            }

        });
    </script>
}