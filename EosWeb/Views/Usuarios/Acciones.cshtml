@using Eosweb.Models
@model List<Eosweb.Models.Log>
@{
    ViewData["Title"] = "Acciones";
    ViewBag.Title = "Acciones";
}

<!-- Datatables -->
<link href="~/lib/select2/dist/css/select2.min.css" rel="stylesheet" />
<link href="~/lib/datatables/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
<link href="~/lib/datatables/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet">
<link href="~/lib/datatables/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css" rel="stylesheet">
<link href="~/lib/datatables/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
<link href="~/lib/datatables/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">


<div class="right_col" role="main">
    <div class="">
        <!--Titulo-->
        <div class="page-title">
            <div class="title_left">
                <h3>@ViewBag.Title</h3>
            </div>
        </div>
        <br />
        <!--/ Titulo-->
        <!--Breadcrumb-->
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Inicio</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Llaves y acciones</li>
                    </ol>
                </nav>
            </div>
        </div>
        <!--/ Breadcrumb-->

        <div class="clearfix"></div>
        <!--Contenido -->

        <div class="">
            <!--Lista -->
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2><i class="fa fa-th-list"></i> </h2>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <!--Content-->
                        <table id="datatable" class="table table-striped table-bordered table-responsive">
                            <thead>
                                <tr>
                                    <th>Fecha y Hora</th>
                                    <th>Autor</th>
                                    <th></th>
                                </tr>
                            </thead>


                            <tbody>
                                @foreach (var d in Model)
                                {
                                <tr>
                                    <td>@d.fecha</td>
                                    <td>@d.autor.Rut</td>
                                    <td width="30%">@d.accion</td>
                                </tr>
                                }
                            </tbody>
                        </table>
                        <!--/ Content-->
                    </div>
                </div>
            </div>
            <!--end Lista-->
        </div>

    </div>
</div>